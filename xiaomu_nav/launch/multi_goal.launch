<?xml version="1.0"?>

<launch>
    <!-- For Simulation -->
    <arg name="sim_mode" default="false" />
    <param name="/use_sim_time" value="$(arg sim_mode)"/>
    
    <!-- move base -->
    <node pkg="xiaomu_nav" type="multi_goals.py" respawn="false" name="multi_goals" output="screen">
        <!-- params for move_base -->
        <param name="goalListX" value="[5.586, 2.276, -3.157]" />
        <param name="goalListY" value="[-15.440, -15.943, -1.408]" />
        <param name="map_frame" value="map" />
    </node>
    

</launch>
